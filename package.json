{
  "name": "me",
  "version": "1.0.0",
  "description": "My personal website",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "prettier  **/*.{js,ts,jsx,tsx} --write",
    "prettier": "prettier --single-quote --write",
    "test": "npm run lint",
    "export": "npm run build && next export"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericcecchi/me.git"
  },
  "keywords": [],
  "author": "Eric Cecchi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericcecchi/me/issues"
  },
  "homepage": "https://github.com/ericcecchi/me#readme",
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@silvenon/remark-smartypants": "^2.0.0",
    "@vercel/speed-insights": "^1.0.12",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "gray-matter": "^4.0.3",
    "next": "^15.0.0",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-capitalize": "^1.1.0",
    "remark-code-titles": "^0.1.2",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
